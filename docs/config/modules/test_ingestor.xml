<?xml version="1.0"?>
<section xmlns="http://docbook.org/ns/docbook" version="5">
  <title>test_ingestor</title>
  <para>A noop ingestor for the test suite.</para>
  <variablelist>
    <varlistentry>
      <term>loader</term>
      <listitem>
        <para>C</para>
      </listitem>
    </varlistentry>
    <varlistentry>
      <term>image</term>
      <listitem>
        <para>test_ingestor.so</para>
      </listitem>
    </varlistentry>
  </variablelist>
  <section>
    <title>Examples</title>
    <example>
      <title>Loading the test_ingestor.</title>
      <para>This example loads the test_ingestor.</para>
      <programlisting>
      &lt;stratcon&gt;
        &lt;modules&gt;
          &lt;module image="test_ingestor" name="test_ingestor" /&gt;
        &lt;/modules&gt;
      &lt;/stratcon&gt;
    </programlisting>
    </example>
  </section>
